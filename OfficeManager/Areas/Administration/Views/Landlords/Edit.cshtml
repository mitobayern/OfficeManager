@using OfficeManager.Areas.Administration.ViewModels.Landlords
@model CreateLandlordViewModel

@{
    ViewData["Title"] = "Edit Landlord";
}

<div style="padding: 5%;">
    <h1 class="display-4" style="margin-bottom: 100px; text-align: center;">Edit Landlord</h1>
    <form asp-area="Administration" asp-controller="Landlords" asp-action="Edit" method="post">
        <partial name="_LandlordPartial" />
        <button type="submit" class="btn btn-primary" style="width:195px">Save Changes</button>
        <a class="btn btn-dark" href="/Administration/Landlords/Details" role="button" style="width:195px">Back</a>
    </form>
</div>